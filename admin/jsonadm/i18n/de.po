#, php-format
msgid "Aimeos object not available"
msgstr "Aimeos-Objekt nicht verfügbar"

#, php-format
msgid "Class \"%1$s\" does not implement \"%2$s\""
msgstr "Die Klasse \"%1$s\" implementiert nicht \"%2$s\""

#, php-format
msgid "Class \"%1$s\" not found"
msgstr "Die Klasse \"%1$s\" ist unbekannt"

#, php-format
msgid "Client generated IDs are not supported"
msgstr "Vom Endgerät generierte IDs werden nicht unterstützt"

#, php-format
msgid "Invalid JSON in body"
msgstr "Ungültiges JSON in der Anfrage"

#, php-format
msgid "Invalid class name \"%1$s\""
msgstr "Der Klassenname \"%1$s\" is ungültig"

#, php-format
msgid "Invalid client \"%1$s\""
msgstr "Unbekannter Client \"%1$s\""

#, php-format
msgid "Invalid client \"%1$s\" in \"%2$s\""
msgstr "Ungültiger Name \"%1$s\" des Clients in \"%2$s\""

#, php-format
msgid "Invalid factory \"%1$s\""
msgstr "Ungültige Factory \"%1$s\""

#, php-format
msgid "No ID given"
msgstr "Keine ID übergeben"

#, php-format
msgid "No view available"
msgstr "Es ist kein View vorhanden"

#, php-format
msgid "Not allowed to access JQAdm \"%1$s\" client"
msgstr "Zugriff auf den JQAdm Client \"%1$s\" verweigert"

#, php-format
msgid "Not allowed to access JsonAdm \"%1$s\" client"
msgstr "Zugriff auf den JsonAdm Client \"%1$s\" verweigert"

msgid "Not implemented, use PATCH instead"
msgstr "Nicht implementiert, benutzen sie stattdessen PATCH"

#, php-format
msgid "Template \"%1$s\" not available"
msgstr "Es wurde keine Vorlage für \"%1$s\" gefunden"

#, php-format
msgid "Unable to call method \"%1$s\""
msgstr "Methode \"%1$s\" kann nicht aufgerufen werden"
